module assignment

imports
  
  Common
 imports  Types
  
context-free start-symbols
  
  Start

context-free sorts

  Start Exp Stmt Attribute Entity 

context-free syntax
  
  Start.Empty = <>
  Start.Statement= <<{Stmt "\n end"}*>>
  Stmt.Entity= <<Entity>>
  Exp= <(<Exp>)> {bracket}
  Exp.Add= <<Exp>+<Exp>>
  Exp.Sub=  <<Exp>-<Exp>>
  
  Exp.Id=ID
  Attribute.Exp =<<ID> :<Type> <Exp>>
  Attribute.Ent= [[ID] -> [Entity_Name]]
  Entity.Decl= <entity <Entity_Name>
                <{Attribute "\n"}*>
                >
